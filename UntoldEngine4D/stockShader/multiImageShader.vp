#version 300 es

in vec4 Vertex;
in vec3 Normal;
in vec2 TextureCoord;

out vec4 vVaryingColor;
uniform mat4 MVPMatrix;
uniform mat3 NormalMatrix;
out mediump vec2 vVaryingTexCoords;

void main(void) 
    { 

    vVaryingTexCoords=TextureCoord;
    gl_Position = MVPMatrix*Vertex;
    
    }